{
	"name": "vscode-echarts-extension",
	"displayName": "vscode extension for ECharts",
	"description": "Autocomplete for ECharts",
	"version": "0.2.3",
	"icon": "images/logo.png",
	"engines": {
		"vscode": "^1.46.0"
	},
	"categories": [
		"Other"
	],
	"publisher": "susiwen8",
	"repository": "https://github.com/susiwen8/echarts-vscode-extension.git",
	"activationEvents": [
		"onCommand:echarts.activate"
	],
	"main": "./dist/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "echarts.activate",
				"title": "Active ECharts Extension"
			},
			{
				"command": "echarts.deactivate",
				"title": "Deactivate ECharts Extension"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "webpack --mode production",
		"webpack": "webpack --mode development",
		"webpack-dev": "webpack --mode development --watch",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"lint": "eslint ./src --ext .ts",
		"lint-fix": "eslint ./src/**/*.ts --fix",
		"pack": "vsce package",
		"publish": "vsce publish"
	},
	"devDependencies": {
		"@types/glob": "^7.1.3",
		"@types/lodash": "^4.14.165",
		"@types/mocha": "^8.0.4",
		"@types/node": "^14.14.10",
		"@types/vscode": "^1.51.0",
		"@typescript-eslint/eslint-plugin": "^2.34.0",
		"@typescript-eslint/parser": "^2.34.0",
		"eslint": "^6.8.0",
		"eslint-config-prettier": "^6.11.0",
		"glob": "^7.1.5",
		"mocha": "^8.2.1",
		"ts-loader": "^8.0.2",
		"typescript": "^4.1.3",
		"vscode-test": "^1.4.0",
		"webpack": "^4.44.1",
		"webpack-cli": "^3.3.12"
	},
	"dependencies": {
		"acorn": "^7.4.0",
		"acorn-walk": "^8.0.0",
		"lodash": "^4.17.20"
	}
}
